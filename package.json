{
  "name": "js-starter-kit",
  "version": "1.0.0",
  "description": "A starting point for js dev",
  "scripts": {
    "predev": "babel-node buildScripts/srcServer-precode.js",
    "dev": "babel-node buildScripts/srcServer.js",
    "start": "npm-run-all --parallel security-check dev",
    "security-check": "nsp check",
    "localtunnel": "lt --port 3000",
    "share": "npm-run-all --parallel localtunnel dev",
    "clean-dist": "rimraf ./dist && mkdir dist",
    "prebuild": "npm run clean-dist",
    "build": "babel-node buildScripts/build.js",
    "postbuild": "babel-node buildScripts/copyAssets.js"
  },
  "author": "Dan Koenig",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^7.1.5",
    "babel-preset-latest": "^6.24.1",
    "babel-register": "^6.26.0",
    "chalk": "^2.4.1",
    "css-loader": "^2.1.0",
    "express": "^4.16.4",
    "npm-run-all": "^4.1.5",
    "open": "0.0.5",
    "path": "^0.12.7",
    "webpack": "^4.28.3",
    "webpack-dev-middleware": "^3.4.0",
    "webpack-hot-middleware": "^2.24.3",
    "webpack-md5-hash": "0.0.6",
    "rimraf": "^2.6.3",
    "whatwg-fetch": "1.0.0",
    "html-webpack-plugin": "^3.2.0",
    "ncp": "^2.0.0",
    "style-loader": "^0.23.1"
  }
}
